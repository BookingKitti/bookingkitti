<form name="Upload_Pics" method="post" action="/uploadPics">
    <div class="modal fade" id=<%- "'Upload_Pics'"%> tabindex="-1" role="dialog" aria-labelledby=
        <%-"'ModalLabel'"%> >
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            &times;
                        </button>
                        <h4 class="modal-title" id="myModalLabel">Upload Hotel</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-10">
                                <input id="fulAvatar" name="fulAvatar" type="file" class="form-control" />
                            </div>
                        </div>
                        <br>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary"> 提交
                        </button>
                    </div>
                </div>
            </div>

    </div>
</form>
