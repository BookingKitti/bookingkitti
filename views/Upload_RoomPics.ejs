<% for (var it=0;it<RoomType.length;it++){ %>
    <div class="modal fade" id=<%- "'"+"./Upload_RoomPics"+it+"'"%> tabindex="-1" role="dialog" aria-labelledby=
        <%-"'ModalLabel'"%> >
            <div class="modal-dialog">
              <form class="form-signin" role="form" action=<%- "uploadRoomPics?Hotel_ID="+HotelInfo[0].Hotel_ID+ "&RoomType="+RoomType[it].Type %> method="post" enctype='multipart/form-data'>
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            &times;
                        </button>
                        <h4 class="modal-title" id="myModalLabel">上传图片</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <fieldset>
                                <input id="fulAvatar" name="fulAvatar" type="file" class="form-control" />
                                <br/>
                            </fieldset>
                        </div>
                        <br>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary"> 提交
                        </button>
                    </div>
                </div>
              </form>
            </div>

    </div>
    <%}%>
